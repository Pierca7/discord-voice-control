{
  "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "lib",
      "baseUrl": ".",
      "declaration": true,
      "paths": {
          "*": [
              "node_modules/*",
          ],
          "bot/*": [
            "src/bot/*"
          ],
          "commands/*": [
            "src/bot/commands/*"
          ]
      }
  },
  "include": [
      "src/**/*"
  ]
}