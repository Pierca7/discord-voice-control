{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "typeRoots": ["../../node_modules/@types/**/*"],
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": ["src/**/*"]
}
