{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "configuration": "./src/configuration",
          "controllers": "./src/controllers",
          "middleware": "./src/middleware",
          "models": "./src/models",
          "services": "./src/services",
          "types": "./src/types"
        }
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}