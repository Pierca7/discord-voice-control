{
  "name": "fictional-umbrella",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "rimraf ./lib && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib/src --extensions \".ts,.js\" --source-maps inline",
    "start": "npm run build && node ./lib/src/bot/index.js",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pierca7/fictional-umbrella.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pierca7/fictional-umbrella/issues"
  },
  "homepage": "https://github.com/Pierca7/fictional-umbrella#readme",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --config prettier.config.js --loglevel silent --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "axios": "^0.19.2",
    "discord.js": "^12.2.0",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^4.2.2",
    "fluent-ffmpeg": "^2.1.2",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "regenerator-runtime": "^0.13.5",
    "websocket-stream": "^5.5.2",
    "ytdl-core": "^2.1.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@spotify/eslint-config-base": "^7.0.0",
    "@spotify/eslint-config-typescript": "^7.0.0",
    "@spotify/prettier-config": "^7.0.0",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/node": "^14.0.5",
    "@types/websocket": "^1.0.0",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.0.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  }
}
