{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "bot": "./src/bot",
          "commands": "./src/bot/commands"
        }
      },
      "@babel/plugin-proposal-class-properties"
    ]
  ]
}